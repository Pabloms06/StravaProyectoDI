<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <TextView
        android:id="@+id/textView4"
        android:layout_width="107dp"
        android:layout_height="33dp"
        android:text="@string/sarah_alder"
        android:textSize="18dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/profile_image"
        android:layout_width="90dp"
        android:layout_height="71dp"
        android:orientation="horizontal"
        android:src="@drawable/logo2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="4dp"
        android:layout_height="27dp"
        android:src="@drawable/logo"
        tools:ignore="MissingConstraints" />


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/august_22_380055_7_43_pm"
        android:textSize="18dp"
        android:textStyle="bold|italic"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="118dp"
        tools:layout_editor_absoluteY="33dp" />

    <ImageView
        android:layout_width="33dp"
        android:layout_height="31dp"
        android:src="@drawable/logo"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="33dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/mount_marathon"
        android:textSize="25dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="71dp"
        tools:ignore="MissingConstraints" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView2"
        android:layout_width="410dp"
        android:layout_height="290dp"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="105dp"
        tools:ignore="MissingConstraints" />

    <TableLayout
        android:id="@+id/Tabla"
        android:layout_width="419dp"
        android:layout_height="273dp"
        android:stretchColumns="1"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="388dp"
        android:gravity="center">

        <TableRow
            android:id="@+id/Cabecera"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/ColumnaAnio"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="@string/Distancia"

                android:textSize="18dp" />

            <TextView
                android:id="@+id/ColumnaCiudad"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:gravity="center"
                android:padding="5px"
                android:text="@string/altitud_ganada"

                android:textSize="18dp" />


        </TableRow>

        <TableRow
            android:id="@+id/SeparadorCabecera"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">


        </TableRow>

        <TableRow
            android:id="@+id/Fila1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/Anio1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="1900"
                android:textStyle="bold"
                android:textSize="35sp" />

            <TextView
                android:id="@+id/Cuidad1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="1264 ms"
                android:textStyle="bold"
                android:textSize="35sp" />


        </TableRow>

        <TableRow
            android:id="@+id/Cabecera2"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tiempo2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="@string/tiempo_total"

                android:textSize="18dp" />

            <TextView
                android:id="@+id/avg"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:padding="5px"
                android:text="@string/avg_power"

                android:textSize="18dp" />


        </TableRow>

        <TableRow
            android:id="@+id/SeparadorCabecera2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">


        </TableRow>

        <TableRow
            android:id="@+id/Fila2"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/tiempo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:text="@string/_22_04"
                android:textStyle="bold"
                android:textSize="35dp" />

            <TextView
                android:id="@+id/avg2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="right"
                android:text="@string/_319"
                android:textStyle="bold"
                android:textSize="35sp" />


        </TableRow>

        <TableRow
            android:id="@+id/Cabecera3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/avg3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="5px"
                android:text="@string/tiempo_total"

                android:textSize="18dp" />

            <TextView
                android:id="@+id/calories2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="right"
                android:padding="5px"
                android:text="@string/avg_power"

                android:textSize="18dp" />


        </TableRow>

        <TableRow
            android:id="@+id/SeparadorCabecera3"
            android:layout_width="match_parent"
            android:layout_height="match_parent">


        <TableRow
            android:id="@+id/Fila3"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content">

        <TextView
            android:id="@+id/speed"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="left"
            android:text="@string/_11_0mi_h"
            android:textStyle="bold"
            android:textSize="30sp" />
        <TextView
            android:id="@+id/calories"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="right"
            android:text="@string/_393"
            android:textStyle="bold"
            android:textSize="35dp" />
        </TableRow>
    </TableLayout>

    <com.google.android.material.chip.Chip
        android:id="@+id/chip4"
        android:layout_width="414dp"
        android:layout_height="65dp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="669dp"
        android:text="@string/view_analysis"
        android:textAlignment="center"
        app:chipBackgroundColor="#FF5722"
        android:textColor="@color/white"
        android:gravity="center"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hoka One One Torrent"
        android:textSize="16dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="51dp"
        tools:layout_editor_absoluteY="734dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="28dp"
        android:layout_height="34dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="734dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>






